<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        <link href="{{ asset('css/base.css') }}"     rel="stylesheet" type="text/css" media="all"></link>
        <link href="{{ asset('css/skeleton.css') }}" rel="stylesheet" type="text/css" media="all"></link>
        <link href="{{ asset('css/layout.css') }}"   rel="stylesheet" type="text/css" media="all"></link>
        
        <link href="http://fonts.googleapis.com/css?family=Lato:400,700" rel="stylesheet" type="text/css" media="all"></link>
        
        {% stylesheets filter='less' output='/css/shop.css' '@ShopMainBundle/Resources/less/shop.less' %}
            <link href="{{ asset_url }}" rel="stylesheet" type="text/css" media="all" />
        {% endstylesheets %}
                
        {% block stylesheets %}{% endblock %}
        
        <script src="{{ asset('js/tabs.js') }}" type="text/javascript"></script>
        {#<script src="{{ asset('js/less-1.1.3.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/jquery-1.6.4.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/jquery-ui-1.8.16.custom.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/jquery.noisy.min.js') }}" type="text/javascript"></script>#}
        
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
