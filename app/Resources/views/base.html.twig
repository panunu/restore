<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        
        <!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
        
        <link href="{{ asset('css/base.css') }}"     rel="stylesheet" type="text/css" media="all"></link>
        <link href="{{ asset('css/skeleton.css') }}" rel="stylesheet" type="text/css" media="all"></link>
        <link href="{{ asset('css/layout.css') }}"   rel="stylesheet" type="text/css" media="all"></link>
        <link href="{{ asset('css/tipsy.css') }}"    rel="stylesheet" type="text/css" media="all"></link>
        
        <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
        
        {% stylesheets filter='less' output='/css/shop.css' '@ShopMainBundle/Resources/less/shop.less' %}
            <link href="{{ asset_url }}" rel="stylesheet" type="text/css" media="all" />
        {% endstylesheets %}
                
        {% block stylesheets %}{% endblock %}
        
        <script src="{{ asset('js/jquery-1.7.1.min.js') }}"           type="text/javascript"></script>
        <script src="{{ asset('js/jquery.tipsy.js') }}"               type="text/javascript"></script>
        <script src="{{ asset('js/slides.min.jquery.js') }}"          type="text/javascript"></script>
        <script src="{{ asset('js/tabs.js') }}"                       type="text/javascript"></script>
        <script src="{{ asset('js/html4+html5/jquery.history.js') }}" type="text/javascript"></script>
        
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
            
        {% javascripts output="/js/shop.js" filter="coffee, ?yui_js"
            '@ShopMainBundle/Resources/coffee/shop.coffee'
            '@ShopProductBundle/Resources/coffee/product.coffee'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
        {% block javascripts %}{% endblock %}
    </body>
</html>
